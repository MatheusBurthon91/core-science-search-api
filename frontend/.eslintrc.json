{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "overrides": [
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins":["react"],
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "eslint:recommended"
    ],
    "rules": {
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "no-underscore-dangle": "off",
        "react/jsx-no-constructed-context-values": "off",
        "import/no-cycle": "off",
        "react/jsx-props-no-spreading": "off"
    }
}
